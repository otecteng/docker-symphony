<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Docker</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
<%= form_for(@docker) do |f| %>    
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Basic Form Elements
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-6">
                            
                                <div class="form-group">
                                    <label>Name</label>
